                    "type": "key-value",
                    "props": {
                      "layout": "vertical",
                      "items": [
                        {
                          "label": "Ganhos",
                          "value": "{{deals.won_this_quarter}}"
                        },
                        {
                          "label": "Perdidos",
                          "value": "{{deals.lost_this_quarter}}"
                        },
                        {
                          "label": "Ticket médio",
                          "value": "{{deals.avg_deal_size | currency:BRL}}"
                        }
                      ]
                    }
                  }
                ]
              }
            ]
          }
        ]
      }
