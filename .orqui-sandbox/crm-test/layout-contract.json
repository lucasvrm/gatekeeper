                    {
                      "value": "inactive",
                      "label": "Inativo"
                    },
                    {
                      "value": "lead",
                      "label": "Lead"
                    }
                  ]
                }
              },
              {
                "id": "filter-source",
                "type": "select",
                "props": {
                  "placeholder": "Origem",
