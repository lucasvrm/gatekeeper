{
  "name": "gatekeeper-api",
  "version": "1.0.0",
  "type": "module",
  "description": "Gatekeeper Backend API - Validation System",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "gatekeeper": "./dist/cli/index.js"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "build:cli": "tsc -p tsconfig.cli.json",
    "cli": "ts-node src/cli/index.ts",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "test": "VITEST_RUN_E2E=true vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run",
    "test:integration": "vitest run test/**/*Integration*.spec.ts",
    "test:e2e": "VITEST_RUN_E2E=true vitest run test/**/*.e2e.spec.ts",
    "lint": "eslint src/"
  },
  "dependencies": {
    "@babel/parser": "^7.26.0",
    "@prisma/client": "^6.2.0",
    "@types/multer": "^2.0.0",
    "acorn": "^8.14.0",
    "boxen": "^8.0.1",
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.7",
    "execa": "^9.5.1",
    "express": "^5.2.1",
    "glob": "^11.0.0",
    "helmet": "^8.0.0",
    "inquirer": "^13.2.0",
    "lodash-es": "^4.17.21",
    "marked": "^15.0.12",
    "marked-terminal": "^7.3.0",
    "minimatch": "^10.0.1",
    "multer": "^2.0.2",
    "nanoid": "^5.0.9",
    "ora": "^9.0.0",
    "p-queue": "^8.0.1",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "simple-git": "^3.27.0",
    "tiktoken": "^1.0.17",
    "ts-morph": "^24.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.28.0",
    "prisma": "^6.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^3.2.4"
  }
}
