{
    "name": "gatekeeper",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "kill": "fuser -k 5000/tcp",
        "build": "tsc -b --noCheck && vite build",
        "typecheck": "tsc --noEmit",
        "typecheck:backend": "npm run typecheck --workspace=gatekeeper-api",
        "typecheck:all": "npm run typecheck && npm run typecheck:backend",
        "lint": "eslint .",
        "test": "cross-env VITEST_RUN_E2E=true vitest run",
        "optimize": "vite optimize",
        "preview": "vite preview",
        "db:generate": "prisma generate --schema=packages/gatekeeper-api/prisma/schema.prisma",
        "db:push": "prisma db push --schema=packages/gatekeeper-api/prisma/schema.prisma",
        "db:migrate": "prisma migrate dev --schema=packages/gatekeeper-api/prisma/schema.prisma"
    },
